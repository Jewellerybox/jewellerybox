<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(catalog) -%>">
  <span class='title'>
    <%= catalog.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.catalogs_catalog_path(catalog),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_catalogs_admin_catalog_path(catalog),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.catalogs_admin_catalog_path(catalog),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => catalog.title),
        :method => :delete %>
  </span>
</li>
